
<script src="~/Scripts/jquery.hiMallDatagrid.js"></script>
<script src="~/Scripts/jquery.himallLinkage.nocreate.js"></script>
<script src="~/Scripts/jquery.placeholder.min.js"></script>
<script src="~/Scripts/bootstrap.autocomplete.js"></script>
<link href="~/Content/bootstrap-datetimepicker.min.css" rel="stylesheet" />
<script src="~/Scripts/bootstrap-datetimepicker.js"></script>
<script src="~/Scripts/bootstrap-datetimepicker.zh-CN.js"></script>
<script>    var val = "@Request.QueryString["status"]";</script>
<script src="~/Areas/SellerAdmin/Scripts/DXL/DoctorService.js?@DateTime.UtcNow"></script>
<script src="~/Scripts/jquery.productSelector.js"></script>
<div class="container">
    <ul class="nav nav-tabs-custom clearfix">

        <li class="active" type="onDown" value="0"><a>正常</a></li>
        <li type="unAudit" value="1"><a>禁用</a></li>
    </ul>
    <div class="search-box">
        <form class="form-inline custom-inline">
            <div class="form-group" filter normal audit saleoff>
                <label class="label-inline w75" for="">账号</label>
                <input class="form-control input-ssm" type="text" id="UserName" placeholder="账号" autocomplete="off">
            </div>

            <div class="form-group">
                <label class="label-inline w75" for="">姓名</label>
                <input class="form-control input-ssm" type="text" id="UserRealName" placeholder="姓名" autocomplete="off">
            </div>
            <div class="form-group">
                <label class="label-inline w75" for="">手机号</label>
                <input class="form-control input-ssm" type="text" id="UserCell" placeholder="手机号" autocomplete="off">
            </div>

            <div class="form-group fg-stock">
                <button type="button" class="btn btn-primary btn-sm" id="searchButton">搜 索</button>
            </div>
        </form>
    </div>
    <table id="list" class="table"></table>
   
    <div class="tabel-operate hidden keep" id="productSelectorBtns" style="display:none">
        <div class="tabel-operate hidden" id="saleOff">
            <a class="btn btn-danger btn-ssm" id="batchClose" style="display:none">批量禁用</a>
            <a class="btn btn-default btn-ssm" id="batchOpen" style="display:none">批量启用</a>
        </div>
        <a id="selectAll" class="btn btn-default btn-ssm" style="display:none" href="javascript:void(0);">批量选择</a>
    </div>
</div>

<div style="display:none" id="publish">
    <wb:publish action="publish" id="wb_publisher"></wb:publish>
</div>
<div class="form-group" id="weixinQrCode" style="display:none">
    <img id="QrCode" />
</div>
<input type="hidden" value="@ViewBag.AuditOnOff" id="VBAO" />

<script type="text/javascript">
    $(function () {
        $('.nav-tabs-custom li').each(function () {
            if ($(this).attr("type") == val) {
                $(this).trigger("click");
            }
        });
    });
</script>