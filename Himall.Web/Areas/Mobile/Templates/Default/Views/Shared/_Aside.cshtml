@{
    List<Himall.DTO.CustomerService> customerservices = ViewBag.CustomerServices;

    var count = customerservices != null ? customerservices.Count : 0;

    var FootMenuList = Himall.Web.Framework.ServiceHelper.Create<Himall.IServices.IWeixinMenuService>().GetFootMenus();
}
<!--_Aside start-->
<div id="_Aside_CustomServices">
    @if (count > 0)
    {
        @helper MeiQiaCS(string entId)
{
        <a class="qq_icon" onclick="commonJS.callMeiQiaCS(@entId)" title="美洽客服"><span class="line1"><img src="/images/meiqia_icon.png" /></span></a>
}
        <div class="service-online css">
            @if (count > 1)
            {
                <div class="s-line" style="visibility:hidden">
                    @foreach (var item in customerservices)
                    {
                        if (item.Tool == Himall.Model.CustomerServiceInfo.ServiceTool.QQ)
                        {
                            <a class="qq_icon" target="_blank" href="http://wpa.qq.com/msgrd?v=3&amp;uin=@item.AccountCode&amp;site=qq&amp;menu=yes" title="@item.Name">
                                <span class="line1" data-value=""><i></i></span>
                            </a>
                        }
                        else if (item.Tool == Himall.Model.CustomerServiceInfo.ServiceTool.MeiQia)
                        {
                            @MeiQiaCS(item.AccountCode)
                        }
                    }
                </div>
                <div class="line-btn" id="two-service">
                    <span class="lb-1"><i></i></span>
                </div>
            }
            else
            {
                if (count == 1)
                {
                    foreach (var item in customerservices)
                    {
                        if (item.Tool == Himall.Model.CustomerServiceInfo.ServiceTool.QQ)
                        {
                            <a class="qq_icon_one" target="_blank" href="http://wpa.qq.com/msgrd?v=3&amp;uin=@item.AccountCode&amp;site=qq&amp;menu=yes" title="@item.Name">
                                <div class="line-btn">
                                    <span class="lb-1"><i></i></span>
                                </div>
                            </a>
                        }
                        else if (item.Tool == Himall.Model.CustomerServiceInfo.ServiceTool.MeiQia)
                        {
                            <a class="qq_icon_one" onclick="commonJS.callMeiQiaCS(@item.AccountCode)" title="美洽客服">
                                <div class="line-btn">
                                    <span class="lb-1"><i></i></span>
                                </div>
                            </a>
                            @*@MeiQiaCS(item.AccountCode)*@
                        }
                    }
                }
            }
        </div>
    }
</div>
<script src="~/Areas/Mobile/Templates/Default/Scripts/_Aside.js"></script>

@if (ViewBag.ShowAside != null)
{
    <div class="wx_aside">
        @if (ViewBag.ShowAside == 1)
        {
            <!-- 更多 -->
            <a href="javascript:;" class="btn_more"></a>
            if (string.IsNullOrWhiteSpace(ViewBag.MAppType))
            {
                <div class="wx_aside_item">
                    @foreach (var footmenu in FootMenuList)
                    {
                        <a href="@footmenu.Url"><img src="@footmenu.MenuIcon" style="width:25px;height:25px;" />@footmenu.Name</a>
                        @*<a href="/@ViewBag.AreaName" class="home">首页</a>
                            <a href="/@ViewBag.AreaName/Vshop" class="vshop">微店</a>
                            <a href="/@ViewBag.AreaName/Category" class="classify">分类</a>
                            <a href="/@ViewBag.AreaName/Cart/Cart" class="f-cart">购物车</a>
                            <a href="/@ViewBag.AreaName/Member/Center" class="user">个人中心</a>*@
                    }
                </div>
            }
            else
            {
                <div class="wx_aside_item" style="top:-140px;">
                    <a href="/@ViewBag.AreaName/Vshop/detail/@ViewBag.MVshopId" class="home">诊所首页</a>
                    <a href="/@ViewBag.AreaName/Cart/Cart?returnUrl=@HttpUtility.UrlEncode(Request.RawUrl)" class="f-cart">购物车</a>
                    <a href="/@ViewBag.AreaName/Member/Center" class="user">个人中心</a>
                </div>
            }
        }
        else if (ViewBag.ShowAside == 2)
        {
            <!-- 返回顶部 -->
            <a href="javascript:;" class="btn_top"></a>
        }
        else
        {
            <!-- 都显示 -->
            <a href="javascript:;" class="btn_more"></a>
            <a href="javascript:;" class="btn_top"></a>
            if (string.IsNullOrWhiteSpace(ViewBag.MAppType))
            {
                <div class="wx_aside_item">
                    @foreach (var footmenu in FootMenuList)
                    {
                        <a href="@footmenu.Url"><img src="@footmenu.MenuIcon" style="width:25px;height:25px;" />@footmenu.Name</a>
                        @*<a href="/@ViewBag.AreaName" class="home">首页</a>
                            <a href="/@ViewBag.AreaName/Vshop" class="vshop">微店</a>
                            <a href="/@ViewBag.AreaName/Category" class="classify">分类</a>
                            <a href="/@ViewBag.AreaName/Cart/Cart" class="f-cart">购物车</a>
                            <a href="/@ViewBag.AreaName/Member/Center" class="user">个人中心</a>*@
                    }
                </div>
            }
            else
            {
                <div class="wx_aside_item" style="top:-140px;">
                    <a href="/@ViewBag.AreaName/Vshop/detail/@ViewBag.MVshopId" class="home">诊所首页</a>
                    <a href="/@ViewBag.AreaName/Cart/Cart?returnUrl=@HttpUtility.UrlEncode(Request.RawUrl)" class="f-cart">购物车</a>
                    <a href="/@ViewBag.AreaName/Member/Center" class="user">个人中心</a>
                </div>
            }
        }
    </div>
}
<!--_Aside end-->