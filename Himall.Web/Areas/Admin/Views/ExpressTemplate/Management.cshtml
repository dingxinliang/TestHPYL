@model IEnumerable<Himall.Core.Plugins.IExpress>
<div>
    <div class="container">
        <ul class="nav nav-tabs-custom clearfix">
            <li class="active"><a href="./Management">快递单模板管理</a></li>
            <li><a href="./Setting">物流设置</a></li>
        </ul>
        <table class="table mt20">
            <thead>
                <tr>
                    <th class="clear-i2">快递公司</th>
                    <th class="td-operate clear-q9">操作</th>
                </tr>
            </thead>
            <tbody>
               @foreach (var template in Model)
               {
                <tr>
                    <td class="clear-i2" >@template.DisplayName</td>
                    <td class="td-operate clear-q10"><span class="btn-a"><a href="Edit?name=@(template.Name)">编辑</a></span></td>
                </tr>
               }
            </tbody>
        </table>
    </div>

</div>

